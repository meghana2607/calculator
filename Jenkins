pipeline
   {
    agent any
    stages
    {

        stage('Git-checkout')
        {
            steps 
            {
               git url:'https://github.com/meghana2607/calculator.git'
               echo"checkout successful";
               }
        }
        stage('Testing Stage') 
        {
            steps 
            {
                echo"Entered Testing Stage"
                bat 'java test_calculator.java'
                echo"confirmation stage"
                bat "java run_tests.java'
           
      
            }
        }
      }
 }
      
